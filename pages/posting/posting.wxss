.footer_tab:nth-child(2) view:first-child{background-image: url(data:image/png;base64,)}
.footer_tab:nth-child(2) view:last-child{color:#4585FF;}
/*nav*/
.nav{display: -webkit-box;width: 100%;overflow: hidden;height:35px;line-height:35px;border-bottom: 20rpx solid #f7f8fd;position: fixed;top: 0;left: 0;background-color: #fff;z-index:1;}
.nav_ok{color:#4988fd !important;border-bottom: 1px solid #4988fd;}
.nav_sub{-webkit-box-flex: 1.0;-webkit-box-pack: center;text-align: center;color:#333;font-size:14px;width:28px;}


/* content */
.swiper-box{margin-top: 100rpx;}
.payment{width: 100%;height: 75rpx;font-size: 26rpx;color: #ff6801;line-height: 75rpx;-webkit-box-pack: end;box-pack: end;display: -webkit-box;display: box;border-bottom: 1rpx solid #e7e7e7;}
.payment :nth-child(1){margin-right: 30rpx;}
.gray{color:#bbbbbb;}


/*project*/
.project{display: box;display: -webkit-box;box-sizing: border-box;-webkit-box-sizing: border-box;width: 100%;overflow: hidden;padding: 38rpx 30rpx;border-bottom: 10rpx solid #F7F8FD;}
.project_img{width: 230rpx;height: 158rpx;margin-right: 30rpx;}
.project_img image{width: 230rpx;height: 158rpx;border-radius: 5rpx;-webkit-border-radius: 5rpx;}
.place{display: -webkit-box;display: box}
.all image{width: 28rpx;height: 28rpx;}
.all {display: box;display: -webkit-box;-webkit-box-flex: 1.0;box-flex: 1.0;font-size: 24rpx;color: #888;margin-top: 10rpx;}
.project_content{-webkit-box-flex: 1.0;box-flex: 1.0;}
.project_title{width: 100%;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;font-size: 34rpx;color: #444;font-weight: bold;-webkit-box-flex: 1.0;box-flex: 1.0;height: 70rpx;}
.meny{display: -webkit-box;display: box;font-size: 24rpx;color: #ccc;text-decoration:line-through;-webkit-box-align: end;box-align: end;}
.all view{margin-left: 10rpx;letter-spacing:3rpx;}
.money{display: -webkit-box;display: box;}
.money view{width: 126rpx;height: 50rpx;text-align: center;line-height: 50rpx;border: 1rpx solid #4988fa;font-size: 26rpx;color: #4988fa;border-radius: 8rpx;}


.content{width: 100%;overflow: hidden;margin-top: 64rpx;}
.my_last{margin-top:100rpx;display: -webkit-box;display:box;-webkit-box-pack: center;box-pack: center;}
.my_last image{width: 340rpx;height: 340rpx;}
.nocontent{font-size: 32rpx;color: #999;text-align: center;}