.project{display: box;display: -webkit-box;box-sizing: border-box;-webkit-box-sizing: border-box;width: 100%;overflow: hidden;border-bottom: 10rpx solid #F7F8Fd;padding: 38rpx 30rpx;}
.project_img{width: 230rpx;height: 158rpx;margin-right: 30rpx;}
.project_img image{width: 230rpx;height: 158rpx;border-radius: 5rpx;-webkit-border-radius: 5rpx;}
.place{display: -webkit-box;display: box}
.all image{width: 28rpx;height: 28rpx;}
.all {display: box;display: -webkit-box;-webkit-box-flex: 1.0;box-flex: 1.0;font-size: 24rpx;color: #888;margin-top: 10rpx;}
.project_content{-webkit-box-flex: 1.0;box-flex: 1.0;}
.project_title{width: 100%;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;font-size: 34rpx;color: #444;font-weight: bold;-webkit-box-flex: 1.0;box-flex: 1.0;height: 70rpx;}
.meny{display: -webkit-box;display: box;font-size: 24rpx;color: #ccc;text-decoration:line-through;-webkit-box-align: end;box-align: end;}
.all view{margin-left: 10rpx;letter-spacing:3rpx;}
.money{display: -webkit-box;display: box;font-size: 40rpx;color: #ff5a0e;}
.money view{font-size:30rpx;}

.all view{margin-left: 10rpx;letter-spacing:3rpx;}
.money{display: -webkit-box;display: box;font-size: 40rpx;color: #ff5a0e;}
.money view{font-size:30rpx;}

.information{width: 100%;height: 160rpx;border-bottom: 1rpx solid #e7e7e7;display: box;display: -webkit-box;-webkit-box-pack: justify;box-pack: justify; display:table;}
.information_name{display: -webkit-box;display: box;font-size: 32rpx;color: #333;}
.information_name :nth-child(2){margin-left: 65rpx;margin-right: 61rpx;}
.id{font-size: 28rpx;color: #888;margin-top: 20rpx}
.information_left{vertical-align:middle;
display:table-cell;padding: 0 0 0 30rpx}
.information_right{vertical-align: middle;display: table-cell;-webkit-box-sizing: content-box;box-sizing: content-box;}


.refund_meny{display: -webkit-box;display: box;width: 100%;height: 94rpx;border-bottom: #e7e7e7 1px solid;}
.refund_meny :nth-child(2){margin-right: 32rpx;line-height: 94rpx;color: #ff5a0e;font-size: 28rpx;}
.refund_meny :nth-child(1){-webkit-box-flex: 1.0; box-flex: 1.0;font-size: 28rpx;color: #333;line-height: 94rpx;margin-left : 30rpx;}
.choose image{width: 30rpx;height: 30rpx;margin-top: 33rpx;margin-right: 30rpx;}
.choose{display: -webkit-box;display: box; font-size: 28rpx;color: #888;}
.refund_reason{display: -webkit-box;display: box;width: 100%;height: 94rpx;border-bottom: 20rpx #f7f8fd solid;}
.refund{font-size: 28rpx;color: #333;line-height: 94rpx;-webkit-box-flex: 1.0;box-flex: 1.0;margin-left: 30rpx;}
.choose :nth-child(1){line-height: 94rpx;}
.al_instructions view{font-size: 26rpx;color: #333;line-height: 48rpx;}
.al_instructions{padding: 33rpx 0 0 30rpx;}
.al_instructions :nth-child(1){font-size: 30rpx;color: black;font-weight: bold}
.button_meny{position: fixed;left: 0;bottom: 0;width: 100%;background: #548FFB;color: #fff;font-size: 34rpx;border-radius: 0;}
