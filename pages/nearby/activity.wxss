/*nav*/
.nav{display: -webkit-box;width: 100%;overflow: hidden;height:35px;
line-height:35px;border-bottom: 1px solid #E7E7E7;}
.nav_ok{color:#6B9BFC !important;border-bottom: 4rpx solid #6B9BFC;}
.nav_sub{-webkit-box-flex: 1.0;-webkit-box-pack: center;text-align: center;color:#333333;font-size:14px;}



.list_img image{width: 88rpx;height: 88rpx;padding: 28rpx 28rpx 28rpx 30rpx;}
.list_all{display: -webkit-box;display: box;width: 100%;height: 145rpx;-webkit-box-sizing: border-box;box-sizing: border-box;}
.list_conten{border-bottom: 1px solid #E7E7E7;display: -webkit-box;display: box;-webkit-box-flex: 1.0;box-flex: 1.0;-webkit-box-sizing: border-box;box-sizing: border-box;}
.title{line-height: 145rpx;font-size: 28rpx;color: #333;width: 62%;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.pople{display: -webkit-box;display: box;margin-top: 10rpx;}
.meny{display: -webkit-box;display: box;-webkit-box-flex: 1.0;box-flex: 1.0;font-size: 26rpx;color: #888;-webkit-box-orient: vertical;box-orient: vertical;-webkit-box-pack: center;box-pack: center;letter-spacing:3rpx}
.meny>view{width: 100%;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.shu{color: #333;}
