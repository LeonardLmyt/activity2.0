.warp{position: relative;width: 100%;margin-bottom: 70rpx}
.big_img{width: 100%;height: 380rpx;}
.warp_conten{position: absolute;bottom: -10%;left: 30rpx;
}
.wapr_name{font-size:  40rpx;color: #fff;font-weight: bold;text-shadow:2rpx 2rpx 2rpx #000;letter-spacing:5rpx;display: -webkit-box;display: box;}
.warp_list{font-size: 24rpx;color: #fff;margin-top: 20rpx;display: box;display: -webkit-box;-webkit-box-align:center;
box-align:center;font-weight: bold;
}
.point{width: 8rpx;height: 8rpx;background: white;border-radius: 50%;display: box;display: -webkit-box;overflow:hidden;margin-left: 10rpx;margin-right: 10rpx;
}
.place{width: 690rpx;height: 100rpx;background: #FFFFFF;margin-top: 38rpx;border-radius: 15rpx; border-bottom:  1px solid #f2f2f2;box-shadow: 0 0 150rpx #999;display: -webkit-box;display: box;-webkit-box-align: center;box-align: center;}
.small_place{width: 160rpx;height: 100%;border-right: 2px solid #f2f4f8;display: -webkit-box;display: box;-webkit-box-align: center;box-align: center;}
.small_place image{width: 30rpx;height: 30rpx;margin-left: 20rpx}
.small_place view{font-size: 28rpx;color: #666;margin-left: 10rpx }
.input image{width: 28rpx;height: 28rpx;margin-right: 10rpx;  }
.input{display: -webkit-box;display: box;font-size: 28rpx;-webkit-box-align: center;box-align: center;margin-left: 30rpx;}
.select image{width: 32rpx;height: 32rpx;}
.select{width: 100%;display: -webkit-box;display: box;padding-left: 30rpx;}
.select_activities{font-size: 30rpx;color: #444;margin-left: 10rpx;line-height: 55rpx;font-weight: bold}
.select_img image{width: 690rpx;height: 320rpx;padding: 26rpx 30rpx;}
.select_img{position: relative;}
.set_out{position: absolute;top: 75%;width: auto;background: #4988fd;color: #fff;font-size: 20rpx;line-height: 50rpx;padding: 0 20rpx;border-radius: 40rpx;left: 56rpx;opacity: 0.8}
.title{font-size: 34rpx;color: #444;padding: 0 30rpx 26rpx 30rpx;}
.time{display: -webkit-box;display: box;padding: 0 0 36rpx 30rpx;-webkit-box-align:center;
box-align:center;font-size: 24rpx;color: #999;-webkit-box-flex: 1.0;box-flex: 1.0;}
.dian{width: 6rpx;height: 6rpx;border-radius: 50%;background: #999;margin-left: 20rpx;margin-right: 20rpx;}
.list_all{width: 100%;height: 220rpx;border-top: 10rpx solid #F7F8FB;display: box;display: -webkit-box;}
.list_all image{width: 230rpx;height: 150rpx;margin-left: 30rpx;}
.right{width: 230rpx;overflow: hidden;position: relative}
.left{-webkit-box-flex: 1.0;box-flex: 1.0;margin-left: 32rpx;}
.all_time{display: -webkit-box;display:box;-webkit-box-align: center;box-align: center;font-size: 24rpx;color: #999;padding: 34rpx 0 44rpx 0; }
.midden{width: 100%;height: 100%;display: -webkit-box;display: box;-webkit-box-align: center;box-align: center;box-sizing: border-box;-webkit-box-sizing: border-box;}
.content{font-size: 34rpx;color: #444;padding: 30rpx 32rpx 0 0;}
.di{position:absolute;
top:10%;
width:auto;
background:#4988fd;
color:#fff;
font-size:20rpx;
line-height:40rpx;
padding:0 25rpx;
border-radius:40rpx;  
left:46rpx;
opacity:0.8;
 }
.chinese{font-size: 36rpx;color: #fff;font-weight: bold;}
.english{font-size: 22rpx;color: #fff;}
.meny{display: -webkit-box;display: box;}
.men{display: -webkit-box;display: box;margin-right: 30rpx}
.all{display: -webkit-box;display: box;-webkit-box-align: center;box-align: center;-webkit-box-flex: 1.0;box-flex: 1.0;}
.meny_small{font-size: 22rpx;color: #FF5A0E;font-weight: bold}
.meny_big{font-size: 32rpx;color: #FF5A0E;font-weight: bold}
.menys{display: -webkit-box;display: box;margin-right: 30rpx;}
.big{display: box;display: -webkit-box;}
.menys :nth-child(1){
  font-size: 22rpx;color: #FF5A0E;font-weight: bold;
}
.menys :nth-child(2){
  font-size: 32rpx;color: #FF5A0E;font-weight: bold;
}

/*region*/
.region{box-sizing: border-box;-webkit-box-sizing: border-box;height: 262rpx;overflow: hidden;padding: 40rpx;background-color: #F7F8FC;white-space:nowrap;}
.region_sub{position: relative;display: inline-block;width: 182rpx;height: 182rpx;margin-right: 20rpx;-webkit-box-align: center;box-align: center;-webkit-box-pack: center;box-pack: center;}
.region_sub image{width: 182rpx;height: 182rpx;}
.region_title{position: absolute;top: 0;left: 0;width: 100%;height: 100%;}
.region_title_sub{display: -webkit-box;display:box;width: 100%;height: 100%;-webkit-box-orient: vertical;box-orient: vertical;color: white;-webkit-box-align: center;box-align: center;-webkit-box-pack: center;box-pack: center;}
.region_title_sub view:nth-child(1){font-size: 36rpx;}
.region_title_sub view:nth-child(2){font-size: 22rpx;}