/*project*/
.project{display: box;display: -webkit-box;box-sizing: border-box;-webkit-box-sizing: border-box;width: 100%;overflow: hidden;border-bottom: 10rpx solid #F7F8Fd;padding: 38rpx 30rpx;}
.project_img{width: 230rpx;height: 158rpx;margin-right: 30rpx;}
.project_img image{width: 230rpx;height: 158rpx;border-radius: 5rpx;-webkit-border-radius: 5rpx;}
.place{display: -webkit-box;display: box}
.all image{width: 28rpx;height: 28rpx;}
.all {display: box;display: -webkit-box;-webkit-box-flex: 1.0;box-flex: 1.0;font-size: 24rpx;color: #888;margin-top: 10rpx;}
.project_content{-webkit-box-flex: 1.0;box-flex: 1.0;}
.project_title{width: 100%;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;font-size: 34rpx;color: #444;font-weight: bold;-webkit-box-flex: 1.0;box-flex: 1.0;height: 70rpx;}
.meny{display: -webkit-box;display: box;font-size: 24rpx;color: #ccc;text-decoration:line-through;-webkit-box-align: end;box-align: end;}
.all view{margin-left: 10rpx;letter-spacing:3rpx;}
.money{display: -webkit-box;display: box;font-size: 40rpx;color: #ff5a0e;}
.money view{font-size:30rpx;}
.midden{border-bottom: 10rpx solid #F7F8FB;}
.headOf_img image{display: -webkit-box;display: box;width:  88rpx;height: 88rpx;margin-bottom: 8rpx;}
.headOf_img{display: -webkit-box;display: box;-webkit-box-align: end;box-align: end;-webkit-box-pack: center;box-pack: center;width:  120rpx;height: 120rpx;background: url(data:image/png;base64, );background-size: 100% 100%;}
.new_img image{width: 120rpx;height: 120rpx;}
.headOf_the{display: -webkit-box;display: box;-webkit-box-pack: center;box-pack: center;padding: 47rpx 0 40rpx 0}
.title{display: -webkit-box;display: box;-webkit-box-pack: center;box-pack: center;font-size: 34rpx;color: #333}
.title :nth-child(2){color: #ff5a0e;margin-left: 12rpx;margin-right: 12rpx;}
.pleat{width: 690rpx;background: #558FFB;margin-top: 46rpx;color: #fff;font-size: 34rpx;border-radius: 8rpx;margin-bottom: 50rpx;}

.list_information{width: 100%;height: 151rpx;border-bottom: 1rpx solid #e7e7e7;}
.name image{display: -webkit-box;display: box;width: 36rpx;height: 36rpx;margin-left: 10rpx;margin-right: 84rpx;}
.name{display: -webkit-box;display: box; -webkit-box-align: center;box-align: center;line-height: 1;font-size: 36rpx;color: #333;padding: 40rpx 0 20rpx 30rpx}
.id{display: -webkit-box;display: box;font-size: 26rpx;color: #888;padding: 0 0 0 30rpx;}
.id :nth-child(2){margin-left: 30rpx;}

.total_amount{width: 100%;height: 92rpx;display: -webkit-box;display: box;border-bottom: 10rpx solid #F7F8FB;line-height: 92rpx;-webkit-box-pack: end;box-pack: end;font-size: 28rpx;color: #333;}
.total_amount :nth-child(2){color: #ff5a0e;margin-right: 32rpx;font-weight: bold}


.big_next{padding: 40rpx 0 0rpx 30rpx;}
.order{display: -webkit-box;display: box;margin-bottom: 40rpx}
.order :nth-child(2){margin-left: 36rpx;color: #888;font-size: 26rpx;}
.order :nth-child(1){color: #444;font-size: 26rpx;}


